<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="NavibarBtnStyle">
        <item name="android:textSize">@dimen/s7</item>
        <item name="android:paddingTop">6.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="ToobarColor" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#FF7700</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
</resources>
